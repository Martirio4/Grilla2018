<?xml version="1.0" encoding="UTF-8"?>
    <shape
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="oval">

        <size
            android:height="20dp"
            android:width="20dp"/>

        <stroke
            android:width="1dp"
            android:color="@color/tile5"/>
        <corners
            android:radius="5dp" />

        <padding
            android:left="16dp"
            android:right="16dp"
            android:top="1dp"
            android:bottom="1dp"/>

    <gradient
        android:angle="45"
        android:gradientRadius="300dp"
        android:startColor="@color/tile3"
        android:endColor="@color/tile5"
        />




    </shape>
